{
  "$schema": "https://schema.tauri.app/config/1.5.0",
  "productName": "UberCalendurr",
  "version": "0.1.0",
  "identifier": "com.ubercalendurr.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "frontend",
    "distDir": "../src-tauri/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "UberCalendurr",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
